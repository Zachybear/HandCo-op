<h1>Listing skill_listings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skill_listings.each do |skill_listing| %>
      <tr>
        <td><%= link_to 'Show', skill_listing %></td>
        <td><%= link_to 'Edit', edit_skill_listing_path(skill_listing) %></td>
        <td><%= link_to 'Destroy', skill_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skill listing', new_skill_listing_path %>
