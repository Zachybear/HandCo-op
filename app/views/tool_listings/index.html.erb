<h1>Listing tool_listings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tool_listings.each do |tool_listing| %>
      <tr>
        <td><%= link_to 'Show', tool_listing %></td>
        <td><%= link_to 'Edit', edit_tool_listing_path(tool_listing) %></td>
        <td><%= link_to 'Destroy', tool_listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tool listing', new_tool_listing_path %>
